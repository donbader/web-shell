# Backend Environment Variables

# Server Configuration
PORT=3000
NODE_ENV=development

# Authentication (Phase 6)
AUTH_ENABLED=false
# GOOGLE_CLIENT_ID=your-client-id.apps.googleusercontent.com
# GOOGLE_CLIENT_SECRET=your-secret
